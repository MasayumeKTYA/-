{
  "easycom": {
    "autoscan": true,
    "custom": {
      "^wd-(.*)": "wot-design-uni/components/wd-$1/wd-$1.vue"
    }
  },
  "pages": [
    {
      "path": "pages/index/index",
      "style": {
        "navigationBarTitleText": "首页",
        "app-plus": {
          "bounce": "none",
          "subNVues": [
            {
              "id": "popup",
              "path": "pages/subNvue/popup",
              "type": "popup",
              "style": {
                "top": "75%",
                "height": "25%",
                "z-index": 99,
                "background": "transparent"
              }
            }
          ]
        }
      }
    },
    {
      "path": "pages/my/my",
      "style": {
        "navigationBarTitleText": "我的",
        "app-plus": {
          "bounce": "none"
        }
      }
    },
    {
      "path": "pages/detail/detail",
      "style": {
        "navigationBarTitleText": "详情",
        "app-plus": {
          "bounce": "none"
        }
      }
    },
    {
      "path": "pages/localFile/localFile",
      "style": {
        "navigationBarTitleText": "本地音乐",
        "app-plus": {
          "bounce": "none"
        }
      }
    },
    {
      "path": "pages/scan/scan",
      "style": {
        "navigationBarTitleText": "扫描音乐",
        "app-plus": {
          "bounce": "none"
        }
      }
    },
    {
      "path": "pages/searchMP/searchMP",
      "style": {
        "navigationBarTitleText": "扫描音乐",
        "app-plus": {
          "bounce": "none"
        }
      }
    }
  ],
  "globalStyle": {
    "navigationBarTextStyle": "black",
    "navigationBarTitleText": "uni-app",
    "navigationStyle": "custom",
    "navigationBarBackgroundColor": "#F8F8F8",
    "backgroundColor": "#F8F8F8"
  }
}
